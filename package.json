    "@types/express": "^5.0.0",
    "@types/figlet": "^1.7.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/response-time": "^2.3.8",
    "@types/swagger-ui-express": "^4.1.7",
    "bun-types": "latest",
    "prettier": "^3.4.2",
    "prisma": "^6.3.1"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@prisma/client": "^6.3.1",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
