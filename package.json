{
  "name": "paint-project",
  "version": "1.0.0",
  "description": "first real project",
  "main": "index.js",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "prisma generate && bun build src/index.ts --outdir ./dist --target=node",
    "start": "bun dist/index.js",
    "build:test": "bun install && bun run build && bun run start",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "bun src/seeders/run-seeders.ts",
    "prisma:flush": "bun src/seeders/flush.ts",
    "monitor:up": "docker-compose -f docker-compose-monitoring.yml up -d",
    "monitor:down": "docker-compose -f docker-compose-monitoring.yml down",
    "sync:local-to-cloud": "bun src/seeders/sync-db.ts local-to-cloud",
    "sync:cloud-to-local": "bun src/seeders/sync-db.ts cloud-to-local"
  },
  "author": "",
  "license": "ISC",
